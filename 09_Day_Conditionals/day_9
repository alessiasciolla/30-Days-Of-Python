age = int(input("Inserisci la tua età: "))
if age >= 18:
print("Hai l'età giusta per guidare.")
else:
years_to_wait = 18 - age
print(f"Devi aspettare ancora {years_to_wait} anni per guidare.")
my_age = 17
your_age = int(input("Inserisci la tua età: "))
if my_age > your_age:
age_difference = my_age - your_age
if age_difference == 1:
print(f"Sono più vecchio di te di {age_difference} anno.")
else:
print(f"Sono più vecchio di te di {age_difference} anni.")
elif my_age < your_age:
age_difference = your_age - my_age
if age_difference == 1:
print(f"Sei più vecchio di me di {age_difference} anno.")
else:print(f"Sei più vecchio di me di {age_difference} anni.")
else:print("Abbiamo la stessa età.")
a = int(input("Enter number one: "))
b = int(input("Enter number two: "))
if a > b:print(f"{a} is greater than {b}")
elif a < b:
print(f"{a} is less than {b}")
else: print(f"{a} is equal to {b}")
score = int(input("Enter the student's score: "))
if 80 <= score <= 100:
grade = 'A'
if 70 <= score < 80:
grade = 'B'
if 60 <= score < 70:
grade = 'C'
if 50 <= score < 60:
grade = 'D'
if 0 <= score < 49:
grade = 'F'
print(f"The student's grade is: {grade}")
if month in ['Settembre', 'Ottobre', 'Novembre']:
season = 'Autunno'
if month in ['Dicembre', 'Gennaio', 'Febbraio']:
season = 'Inverno'
if month in ['Marzo', 'Aprile', 'Maggio']:
season = 'Primavera'
if month in ['Giugno', 'Luglio', 'Agosto']:
season = 'Estate'
fruits = ['banana', 'orange', 'mango', 'lemon']
new_fruit = input("Enter a fruit: ").lower()
if new_fruit in fruits:
print("Quel frutto esiste già nella lista")
else:
fruits.append(new_fruit)
print("La lista modificata è:", fruits)
